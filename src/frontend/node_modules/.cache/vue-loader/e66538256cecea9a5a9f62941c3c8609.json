</option>\n                    </select>\n                </CCol>\n                <CCol sm=\"2\">\n                    <select v-model=\"selChargeStatus\" class=\"form-control\">\n                        <option value=\"All\">전체</option>\n                        <option value=\"I\">완납</option>\n                        <option value=\"O\">미납</option>\n                    </select>\n                </CCol>\n                <CCol sm=\"2\">\n                    <CInput\n                            placeholder=\"이름 또는 진료번호 검색\"\n                            v-model=\"searchVal\"\n                    />\n                </CCol>\n                <CCol sm=\"0.5\">\n                    <button\n                            label=\"\"\n                            type=\"button\"\n                            class=\"btn btn-info btn-block\"\n                            @click=\"searchList\">검색</button>\n                </CCol>\n            </CRow>\n        </form>\n        <CRow>\n            <CCol sm=\"12\">\n                <div class=\"card\">\n                    <div class=\"card-body\">\n                        <div bordered=\"true\" small=\"true\">\n                            <div class=\"position-relative table-responsive\">\n                                <b-table\n                                        id = \"list-table\"\n                                        :items=\"newItems\"\n                                        :fields=\"fields\"\n                                        :per-page=\"perPage\"\n                                        :current-page=\"currentPage\"\n                                        striped hover\n                                        @row-clicked=\"goUpdatePage\"\n                                />\n                            </div>\n                            <b-pagination\n                                    :link-gen=\"linkGen\"\n                                    :number-of-pages=\"10\"\n                                    use-router\n                                    v-model=\"currentPage\"\n                                    :total-rows=\"rows\"\n                                    :per-page=\"perPage\"\n                                    aria-controls=\"list-table\"\n                            />\n                        </div>\n                    </div>\n                </div>\n            </CCol>\n        </CRow>\n        <CRow>\n            <CCol class=\"btn-col-margin\" sm=\"0.7\">\n                <div class=\"m-2 d-inline-block dropdown\">\n                    <CCol>\n                        <CDropdown\n                                toggler-text=\"환자 등록\"\n                                color=\"primary\"\n                                class=\"m-2 d-inline-block\"\n                        >\n                            <CDropdownItem @click=\"goCustInsert\">1인 등록</CDropdownItem>\n                            <CDropdownItem @click=\"goExcelUpload\">대량 등록</CDropdownItem>\n                        </CDropdown>\n                    </CCol>\n                </div>\n            </CCol>\n\n            <CCol sm=\"9\"></CCol>\n            <CCol class=\"btn-col-margin\" sm=\"0.7\">\n                    <button label=\"\"\n                            type=\"button\"\n                            style=\"text-align: right\"\n                            class=\"btn btn-info btn-block\">엑셀 다운로드</button>\n            </CCol>\n        </CRow>\n    </div>\n</template>\n<style>\n    .btn-col-margin{\n        margin-top: 5px;\n        margin-left: 15px;\n        margin-right: 10px;\n    }\n</style>\n<script>\n    import axios from 'axios'\n\n    export default {\n        name: 'app',\n        components: {},\n        data: function () {\n            return {\n                perPage: 10,\n                currentPage: 1,\n                newItems: [],\n                fields: [\n                    {\n                        key: 'id',\n                        label: 'No',\n                        sortable: true\n                    },\n                    {\n                        key: 'name',\n                        label: '이름',\n                        sortable: true\n                    },\n                    {\n                        key: 'registNo',\n                        label: '진료번호',\n                        sortable: true\n                    },\n                    {\n                        key: 'custStatus',\n                        label: '상태',\n                        sortable: true\n                    },\n                    {\n                        key: 'inDate',\n                        label: '입원일',\n                        sortable: true\n                    },\n                    {\n                        key: 'outDate',\n                        label: '퇴원일',\n                        sortable: true\n                    },\n                    {\n                        key: 'chargeStatus',\n                        label: '납부상태',\n                        sortable: true\n                    },\n                    {\n                        key: 'charge',\n                        label: '미납액',\n                        sortable: true\n                    }\n                ],\n                items: null,\n                searchVal: null,\n                selCustStatus: 'All',\n                selChargeStatus: 'All'\n            }\n        },\n        computed: {\n            rows() {\n                return this.newItems.length;\n            }\n        },\n        mounted() {\n            axios.get('silver/cust/getList')\n                .then(res => {\n                    this.items = res.data;\n                    console.log(this.items)\n\n                    // 입퇴원 상태 변환   ex) O -> 입원\n                    for(var i = 0; i < this.items.length; i++){\n                        const obj = new Object();\n                        if(this.items[i].custStatus === \"O\"){\n                            obj.custStatus = \"입원\";\n                        } else {\n                            obj.custStatus = \"퇴원\";\n                        }\n\n                        if(this.items[i].charge === 0){\n                            obj.chargeStatus = \"완납\"\n                        } else {\n                            obj.chargeStatus = \"미납\";\n                            obj._rowVariant = \"danger\";\n                        }\n\n                        obj.id = this.items[i].id;\n                        obj.name = this.items[i].name;\n                        obj.registNo = this.items[i].registNo;\n                        obj.inDate = this.items[i].inDate;\n                        obj.outDate = this.items[i].outDate;\n                        obj.charge = this.items[i].charge;\n\n                        this.newItems.push(obj);\n                    }\n                    console.log(this.newItems)\n\n                })\n                .catch(function(error) {\n                    console.log(error);\n                });\n\n            this.newItems.forEach(function(obj){\n                if(obj.chargeStatus ===\"미납\"){\n                    console.log(\"미납 in\")\n                    console.log(obj.chargeStatus);\n                    obj._rowVariant = \"success\";\n                }\n            })\n\n        },\n        methods: {\n            searchList(value){\n                const qs = require('qs');\n                const formData = qs.stringify({'searchVal': this.searchVal\n                    ,'selCustStatus': this.selCustStatus\n                    ,'selChargeStatus': this.selChargeStatus\n                })\n\n                axios.post('silver/cust/getList', formData)\n                    .then(res => {\n                        this.items = res.data;\n                        console.log(this.items)\n                        console.log('검색통신완료')\n\n                        this.newItems = [];\n\n                        // 입퇴원 상태 변환   ex) O -> 입원\n                        for(var i = 0; i < this.items.length; i++){\n                            const obj = new Object();\n                            if(this.items[i].custStatus === \"O\"){\n                                obj.custStatus = \"입원\";\n                            } else {\n                                obj.custStatus = \"퇴원\";\n                            }\n\n                            if(this.items[i].charge === 0){\n                                obj.chargeStatus = \"완납\"\n                            } else {\n                                obj.chargeStatus = \"미납\";\n                                obj._rowVariant = \"danger\";\n                            }\n\n                            obj.id = this.items[i].id;\n                            obj.name = this.items[i].name;\n                            obj.registNo = this.items[i].registNo;\n                            obj.inDate = this.items[i].inDate;\n                            obj.outDate = this.items[i].outDate;\n                            obj.charge = this.items[i].charge;\n\n                            this.newItems.push(obj);\n                        }\n                        console.log('newItem')\n                        console.log(this.newItems)\n\n                    })\n                    .catch(function(error) {\n                        console.log(error);\n                    });\n            },\n\n            goCustInsert() {\n                this.$router.push({name: 'paymanageinsert'})\n            },\n\n            goExcelUpload(){\n                this.$router.push({name: 'excelupload'})\n            },\n\n            linkGen(pageNum) {\n                return pageNum === 1 ? '?' : `?page=${pageNum}`\n\n            },\n\n            goUpdatePage(record, index) {\n                this.$router.push({name: 'paymanageupdate', params: {custId: record.id}})\n                console.log(record.id)\n                console.log(index)\n            }\n        }\n    }\n</script>"]}]}