                    this.custChargeItem = res.data.custCharge;\r\n                    this.preInDate = res.data.cust.inDate.slice(0, 10);\r\n                    this.preOutDate = res.data.cust.outDate.slice(0, 10);\r\n                    this.preInsertDate = res.data.cust.insertDate.slice(0, 10);\r\n                    this.selected = res.data.cust.custStatus;\r\n                    console.log(\"custPayItem\")\r\n                    console.log(this.custPayItem)\r\n                    console.log(\"custChargeItem\")\r\n                    console.log(this.custChargeItem)\r\n                })\r\n                .catch(function(error) {\r\n                    console.log(error);\r\n                });\r\n        },\r\n        methods: {\r\n            submit () {\r\n                axios.post('/silver/cust/custUpdate', {\r\n                    id: this.$route.params.custId,\r\n                    name: this.custItem.name,\r\n                    registNo: this.custItem.registNo,\r\n                    custStatus: this.selected,\r\n                    inDate: this.preInDate,\r\n                    outDate: this.preOutDate,\r\n                    charge: this.custItem.charge\r\n                })\r\n                    .then(function (res) {\r\n                        console.log(res.data)\r\n                    })\r\n                    .catch(function (error) {\r\n                        console.log(error)\r\n                    });\r\n            }\r\n        }\r\n    }\r\n</script>\r\n"]}]}